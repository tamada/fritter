{
  "name": "strict",
  "validators": [
    "indent_level","no_else",
    "primitive_wrapping",
    "dot_count_per_line",
    "no_abbrev",
    "lines_of_file",
    "lines_of_method",
    "classes_in_package",
    "field_count",
    "first_class_collection",
    "no_accessor",
    "variable_count"
  ],
  "parameters": {
    "indent_level": 1,
    "dot_count_per_line": 1,
    "lines_of_file": 50,
    "lines_of_method": 3,
    "classes_in_package": 10,
    "field_count": 2,
    "variable_count": 2
  }
}
